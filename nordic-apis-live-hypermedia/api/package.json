{
  "name": "@tpluscode/hypermedia-live-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "bootstrap": "run-s bootstrap:*",
    "bootstrap:resources": "talos put --api $API_BASE --endpoint $DB_URL",
    "bootstrap:vocabs": "talos put-vocabs --apiDoc $API_BASE/api --endpoint $DB_URL --extraVocabs @zazuko/rdf-vocabularies,schema",
    "start": "knossos serve $DB_URL?infer=true"
  },
  "dependencies": {
    "@hydrofoil/knossos": "^0.2.5",
    "@hydrofoil/talos": "^0.3.4"
  },
  "devDependencies": {
    "dotenv-cli": "^4.0.0",
    "npm-run-all": "^4.1.5"
  }
}
